<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <tndn.app.nyam.widget.TDActionBar
        android:id="@+id/actionbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true" />

    <TextView
        android:id="@+id/map_public_rout_result_totalcount"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/actionbar"
        android:background="@color/white"
        android:gravity="center"
        android:padding="5dp"
        android:text="검색결과 총 10개"
        android:textSize="10sp" />

    <ListView
        android:id="@+id/map_public_rout_result_listview"
        android:layout_width="match_parent"
        android:dividerHeight="10dp"
        android:divider="@color/transparent"
        android:layout_height="match_parent"
        android:layout_below="@id/map_public_rout_result_totalcount" />

</RelativeLayout>